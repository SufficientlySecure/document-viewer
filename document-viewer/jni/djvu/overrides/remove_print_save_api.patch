-                complain(uarg,"Invalid number of copies.");
-              options.set_copies(n);
-            }
-          else if (s == "frame")
-            {
-              if (arg == "yes" || arg == "")
-                options.set_frame(true);
-              else if (arg == "no")
-                options.set_frame(false);
-              else
-                complain(uarg,"Invalid argument. Use \"yes\" or \"no\".");
